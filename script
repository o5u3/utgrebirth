game.Players.LocalPlayer:Kick("UTG Re-Birth is discontinued. Join for FE R15 Hub: discord.gg/tU87b6w")
while wait() do
game:FindService("StarterGui"):SetCore("SendNotification", {
        Title = "UTG Re-Birth",
        Text = "This hub is discontinued.",
        Duration = 5
    })
game:FindService("StarterGui"):SetCore("SendNotification", {
        Title = "UTG Re-Birth",
        Text = "Join for FE R15 Hub: discord.gg/tU87b6w",
        Duration = 5
    })
end
